/*
VSCE: ESLint
VSCE: Sort JS object keys
VSCE: JavaScript and TypeScript Nightly
*/
{
	"env": {
		"browser": true,
		"es2021": true,
		"node": true
	},
	"extends": [ "plugin:lube/recommended" ],
	"ignorePatterns": [
		"**/types/**/*.d.ts",
		"**/*.cjs",
		"coverage/"
	],
	"overrides": [
		{
			"files": [ "**/*.d.ts" ],
			"rules": { "no-unused-vars": "off" }
		}
	],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": [ "lube" ],
	"rules": {}
}